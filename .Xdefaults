!-- Urxvt --!
! ctrl-insert = copy
! shift-insert = paste

URxvt.font:                xft:Terminus:size=14
URxvt.scrollBar:           false
URxvt.internalBorder:      2
URxvt.geometry:            86x23
URxvt.urgentOnBell:        true
URxvt.cursorBlink:         false
URxvt.perl-ext-common:     default,matcher,url-picker
URxvt.keysym.C-0xff0d:     perl:url-picker
URxvt.url-picker.launcher: /usr/bin/firefox
URxvt.url-launcher:        /usr/bin/firefox
urxvt.saveLines:           1000000
URxvt.matcher.button:      C1
URxvt.colorUL:             #859900

!-- Theme --!
*background: #00222C
*foreground: #93a1a1
*cursorColor: #93a1a1

!! black dark/light
*color0: #002b36
*color8: #657b83
!! red dark/light
